// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}


model Reservation {
  id        Int     @id @default(autoincrement())
  createdAt DateTime @default(now())
  plage     DateTime
  nom         String
  courriel    String  
  marque      String
  annee       String
  modele      String
}